[Oracle]
type=driver
server=localhost
port=1521
database=SID
url=jdbc:oracle:thin:@%s:%p:%d
class=oracle.jdbc.driver.OracleDriver

[MySQL]
type=driver
server=localhost
database=mysql
url=jdbc:mysql://%s/%d
class=org.gjt.mm.mysql.Driver

[SQL Server]
type=driver
server=localhost
port=1433
database=master
url=jdbc:inetdae7:%s:%p?database=%d
class=com.inet.tds.TdsDriver

[PostgreSQL]
type=driver
server=localhost
port=5432
database=db
url=jdbc:postgresql://%s:%p/%d
class=org.postgresql.Driver

[DriverManager]
type=driverManager
urls=/usr/share/java/postgresql.jar
